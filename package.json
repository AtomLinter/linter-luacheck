{
  "name": "linter-luacheck",
  "description": "Lint Lua on the fly, using luacheck",
  "keywords": [
    "linter",
    "lua"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "main": "./lib/init",
  "version": "1.1.4",
  "repository": "https://github.com/AtomLinter/linter-luacheck",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "atom-linter": "^4.7.0",
    "atom-package-deps": "latest"
  },
  "package-deps": [
    "linter"
  ],
  "devDependencies": {
    "coffeelint": "^1.15.7"
  },
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib spec"
  }
}
